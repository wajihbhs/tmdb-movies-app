{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "types": ["@nuxt/schema", "@nuxt/types", "node"],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
